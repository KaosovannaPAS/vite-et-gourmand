{
    "version": 2,
    "functions": {
        "noyau_backend/api/v1/*.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "noyau_backend/api/v1/user/*.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "api/**/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/noyau_backend/api/v1/(.*)",
            "dest": "/noyau_backend/api/v1/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}