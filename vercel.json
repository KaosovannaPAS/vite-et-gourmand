{
    "version": 2,
    "functions": {
        "v.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "noyau_backend/api/v1/*.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "noyau_backend/api/v1/auth/*.php": {
            "runtime": "vercel-php@0.7.2"
        },
        "noyau_backend/api/v1/user/*.php": {
            "runtime": "vercel-php@0.7.2"
        }
    },
    "routes": [
        {
            "src": "/v.php",
            "dest": "/v.php"
        },
        {
            "src": "/noyau_backend/(.*)",
            "dest": "/noyau_backend/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}