{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php@0.7.2"
        }
    ],
    "routes": [
        {
            "src": "/interface_frontend/(.*)",
            "dest": "/interface_frontend/$1"
        },
        {
            "handle": "filesystem"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}