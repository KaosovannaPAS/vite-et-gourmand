{
    "version": 2,
    "builds": [
        {
            "src": "index.php",
            "use": "vercel-php@0.7.2"
        },
        {
            "src": "test_db.php",
            "use": "vercel-php@0.7.2"
        }
    ],
    "routes": [
        {
            "src": "/test_db.php",
            "dest": "/test_db.php"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ]
}