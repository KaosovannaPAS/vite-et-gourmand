{
    "version": 2,
    "builds": [
        {
            "src": "**/*.php",
            "use": "vercel-php@0.7.2"
        },
        {
            "src": "index.html",
            "use": "@vercel/static"
        },
        {
            "src": "interface_frontend/ressources/**/*",
            "use": "@vercel/static"
        },
        {
            "src": "interface_frontend/pages/*.html",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*).php",
            "dest": "/$1.php"
        }
    ]
}